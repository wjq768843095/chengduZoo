<view class="container">
    <view class="zan-panel">
        <form bindsubmit="addFeedback">
            <view class="zan-cell zan-form__item">
                <text class="zan-form__title">联系方式：</text>
                <input class="zan-form__input zan-cell__bd" maxlength="11" name="contact" placeholder="请输入联系方式" type="number" value="{{contact}}"></input>
            </view>
            <view class="zan-cell zan-form__item" style="align-items:flex-start">
                <text class="zan-form__title">建议内容：</text>
                <textarea class="zan-form__input zan-cell__bd" maxlength="200" name="content" placeholder="请输入建议" type="textarea" value="{{content}}"></textarea>
            </view>
            <view class="zan-btns">
                <button class="zan-btn zan-btn--primary" formType="submit">立即提交</button>
            </view>
        </form>
    </view>
    <view class="page__ft">
        <view class="page__bd page__bd_spacing">
            <view class="weui-footer ">
                <view class="weui-footer__text" style="font-size:12px">Copyright © 2013-2018 www.cdusc.cn</view>
            </view>
        </view>
    </view>
</view>
